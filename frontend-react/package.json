  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/styles": "^4.11.5",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/react": "^14.0.0",
    "@transifex/native": "^5.1.0",
    "@transifex/react": "^5.1.0",
    "axios": "^1.3.4",
    "case-converter": "^1.0.1",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "jss": "^10.10.0",
    "jss-rtl": "^0.3.0",
    "jwt-decode": "^3.1.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "react-world-flags": "^1.5.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.7",
    "web-vitals": "^3.1.1",
    "yup": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "babel-plugin-i18next-extract": "^0.9.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-destructuring-newline": "0.0.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n-push": "txjs-cli push --verbose --purge src/",
    "i18n-push:dry": "txjs-cli push --verbose --purge --dry-run src/",
    "i18n-invalidate": "txjs-cli invalidate",
    "lint": "eslint --quiet \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint --quiet \"src/**/*.{js,jsx}\" --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint"
    ],
    "*.jsx": [
      "yarn eslint"
    ]
  }
}
